//YKProg_G_c_ch2
#include <stdio.h>

int main()
{
    int prem,age,pol;
    char g,h,l;
    printf("\n Enter Your Gender [M/F] : ");
    scanf("%s",&g);
    printf("\n Enter Your Doctor's Health Diagonosis [Excellent(E)/Poor(P)] : ");
    scanf("%s",&h);
    printf("\n Enter Your Area of residence [Rural(R)/Urban(U)] : ");
    scanf("%s",&l);
    printf("\n Enter your age :");
    scanf("%d",&age);
    printf("\n Enter your policy amount :");
    scanf("%d",&pol);
    if (h=='E' && age>=25 && age<=35 && l=='U' && g=='M' && pol<=200000) 
     printf("\n Your Policy Premium is Rs 4000");
    else if (h=='E' && age>=25 && age<=35 && l=='U' && g=='F' && pol<=100000) 
    printf("\n Your Policy Premium is Rs 3000");
    else if (h=='P' && age>=25 && age<=35 && l=='R' && g=='M' && pol<=10000) 
    printf("\n Your Policy Premium is Rs 4000");
    else printf("\n You are not eligible for Policy");
 return 0;
}
        

//YKProg_G_d_ch2
#include <stdio.h>

int main()
{
    int h,ts;
    float cc;
    printf("\n Enter the Hardness Carbon Content and Tensile strength of Steel : ");
    scanf("%d%f%d",&h,&cc,&ts);
    if (h>50 && cc<0.7 && ts>5600) printf("The steel is grade 10");
    else if (h>50 && cc<0.7) printf("The steel is grade 9");
    else if (cc<0.7 && ts>5600) printf("The steel is grade 8");
    else if (h>50 && ts>5600) printf("The steel is grade 7");
    else if (h>50 || cc<0.7 || ts>5600) printf("The steel is grade 6");
    else printf("The steel is grade 5");
    return 0;
}





